<entry_list version="1.0">
	<entry id="digital"><ew>digital</ew><subj>AN-1#MD-2#MA-3#NB-3#MA-4#NB-4#MA-5#NB-5#AU-6#CP-3#CP-4#TL-4#CP-7#EE-7</subj><hw>dig*i*tal</hw><sound><wav>digita01.wav</wav><wpr>!di-ju-t/ul</wpr></sound><pr>ˈdi-jə-tᵊl</pr><fl>adjective</fl><et>Latin <it>digitalis</it></et><def><date>circa 1656</date> <sn>1</sn> <dt>:of or relating to the fingers or toes <vi><it>digital</it> dexterity</vi></dt> <sn>2</sn> <dt>:done with a finger <vi>a <it>digital</it> rectal examination</vi></dt> <sn>3</sn> <dt>:of, relating to, or using calculation by numerical methods or by discrete units</dt> <sn>4</sn> <dt>:of, relating to, or being data in the form of especially binary <fw>digits</fw> <vi><it>digital</it> images</vi> <vi>a <it>digital</it> readout</vi></dt> <sd>especially</sd> <dt>:of, relating to, or employing digital communications signals <vi>a <it>digital</it> broadcast</vi> <dx>compare <dxt>analog <dxn>2</dxn></dxt></dx></dt> <sn>5</sn> <dt>:providing a readout in numerical digits <vi>a <it>digital</it> voltmeter</vi></dt> <sn>6</sn> <dt>:relating to an audio recording method in which sound waves are represented digitally (as on magnetic tape) so that in the recording wow and flutter are eliminated and background noise is reduced</dt> <sn>7</sn> <dt>:<sx>electronic</sx> <vi><it>digital</it> devices</vi></dt> <sd>also</sd> <dt>:characterized by electronic and especially computerized technology <vi>the <it>digital</it> age</vi></dt></def><uro><ure>dig*i*tal*ly</ure><sound><wav>digita02.wav</wav><wpr>!di-ju-t/ul-E</wpr></sound> <pr>-tᵊl-ē</pr> <fl>adverb</fl></uro></entry>
	<entry id="digital camera"><ew>digital camera</ew><subj>CP#PX</subj><hw>digital camera</hw><fl>noun</fl><def><date>1976</date><dt>:a camera that records images as digital data instead of on film</dt></def></entry>
	<entry id="digital computer"><ew>digital computer</ew><subj>CP</subj><hw>digital computer</hw><fl>noun</fl><def><date>1947</date><dt>:a computer that operates with numbers expressed directly as digits <dx>compare <dxt>analog computer</dxt> <dxt>hybrid computer</dxt></dx></dt></def></entry>
	<entry id="digital subscriber line"><ew>digital subscriber line</ew><grp>digital subscriber</grp><grp>digital line</grp><grp>subscriber line</grp><subj>CP#TL</subj><hw>digital subscriber line</hw><fl>noun</fl><def><date>1984</date><dt>:a high-speed communications connection used for accessing the Internet and carrying short-range transmissions over ordinary telephone lines</dt></def></entry>
	<entry id="digital versatile disc"><ew>digital versatile disc</ew><grp>digital versatile</grp><grp>digital disc</grp><grp>versatile disc</grp><subj>CP#TL</subj><hw>digital versatile disc</hw><fl>noun</fl><def><date>1995</date><dt>:<sx>dvd</sx></dt></def></entry>
	<entry id="digital video disc"><ew>digital video disc</ew><grp>digital video</grp><grp>digital disc</grp><grp>video disc</grp><subj>CP#TL</subj><hw>digital video disc</hw><fl>noun</fl><def><date>1993</date><dt>:<sx>dvd</sx></dt></def></entry>
	<entry id="personal digital assistant"><ew>personal digital assistant</ew><grp>personal digital</grp><grp>personal assistant</grp><grp>digital assistant</grp><subj>CP</subj><hw>personal digital assistant</hw><fl>noun</fl><def><date>1992</date><dt>:<sx>pda</sx></dt></def></entry>
